    "_comment": "TEST VECTOR DOCUMENTATION",
    "_description": "Comprehensive test vectors for Lanchester Linear Law model validation",
    "_model": "LanchesterLinear",
    "_formula": "A(t) = A₀ - β·t, B(t) = B₀ - α·t",
    "_invariant": "linear_advantage = α·A₀ - β·B₀",
    "_total_tests": 14,
    "_coverage": {
      "symmetric_scenarios": [
        "equal_forces_equal_effectiveness_draw - Perfect symmetry leading to mutual elimination",
        "very_small_forces - Tests numerical stability with fractional forces"
      ],
      "asymmetric_victories": [
        "B_wins_less_effective_more_numerous - Numerical advantage overcomes lower effectiveness",
        "A_wins_small_but_much_more_effective - Superior effectiveness overcomes numerical disadvantage",
        "A_wins_more_effective_coefficient - Higher effectiveness coefficient determines victory",
        "A_wins_extreme_coefficients - Tests extreme coefficient ratios (α=9.0, β=0.25)"
      ],
      "degenerate_cases": [
        "degenerate_alpha_zero - Force A has no offensive capability (α=0)",
        "degenerate_beta_zero - Force B has no offensive capability (β=0)"
      ],
      "near_draw_scenarios": [
        "near_draw_beta_slightly_lower - β=0.99 vs α=1.0, minimal winner advantage",
        "near_draw_alpha_slightly_lower - α=0.99 vs β=1.0, tests numerical precision"
      ],
      "boundary_conditions": [
        "zero_A_initial - Force A starts with zero strength (instant loss)",
        "zero_B_initial - Force B starts with zero strength (instant win)",
        "both_zero_initial - Both forces start at zero (edge case)"
      ],
      "general_cases": [
        "asymmetric_large_alpha - Large alpha (10.0) with small beta (0.5)",
        "fractional_survivors - Tests scenarios with small remaining forces"
      ]
    },
    "_test_structure": {
      "inputs": "A0, B0, alpha, beta (initial forces and effectiveness coefficients)",
      "expected": "Af, Bf, T, winner, linear_advantage (final state)",
      "snapshots": "4-5 temporal samples showing force evolution during battle",
      "tolerance": "Numerical comparison tolerances (abs: 1e-9, rel: 1e-9)"
    },
    "_validation_notes": [
      "All values calculated from first principles using Linear Law formulas",
      "No values derived from model implementation to avoid circular validation",
      "Linear advantage (α·A₀ - β·B₀) remains constant throughout battle",
      "Battle ends when first force reaches zero (or both simultaneously for draws)",
      "Time T represents battle duration until elimination"
    ],
    "_usage": "Load this file to validate LanchesterLinear implementation correctness, regression testing, and cross-implementation verification"
